/*
 ▼吹き出し
--------------------------------------------------------------------------------*/
/*yell*/
.ballone{
    background: #fff;
    padding:10px 5px;
    border: 1px solid #e0d7b0;
    position: relative;
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    color:#4d3e21;
    font-size:72%;
    -webkit-box-shadow: 0px 2px 2px rgba(255,255,255,0.6);
    -moz-box-shadow:0px 2px 2px rgba(255,255,255,0.6);
    box-shadow:0px 2px 2px rgba(255,255,255,0.6);
    background: -moz-linear-gradient(top,  #fff 0%, #f8f8f8 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fff), color-stop(100%,#f8f8f8));
    background: -webkit-linear-gradient(top,  #fff 0%,#f8f8f8 100%);
    background: -o-linear-gradient(top,  #fff 0%,#f8f8f8 100%);
    background: -ms-linear-gradient(top,  #fff 0%,#f8f8f8 100%);
    background: linear-gradient(to bottom,  #fff 0%,#f8f8f8 100%);
}
.ballone:after, .ballone:before {
    bottom: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}
.ballone:after {
    border-color: rgba(255, 255, 255, 0);
    border-bottom-color: #fff;
    border-width: 5px;
    left: 30px;
    margin-left: -7px;
}
.ballone:before {
    border-color: rgba(0, 0, 0, 0);
    border-bottom-color: #e0d7b0;
    border-width: 7px;
    left: 30px;
    margin-left: -9px;
}
/*pink */
.ballone_pnk{
	width: 100%;
    background: #fff8f8;
    padding:7px;
    border: 1px solid #ffc4c4;
    position: relative;
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    color:#fa8c8c;
}
.ballone_pnk:after, .ballone_pnk:before {
    bottom: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}
.ballone_pnk:after {
    border-color: rgba(255, 255, 255, 0);
    border-bottom-color: #fff8f8;
    border-width: 5px;
    left: 30px;
    margin-left: -7px;
}
.ballone_pnk:before {
    border-color: rgba(0, 0, 0, 0);
    border-bottom-color: #ffc4c4;
    border-width: 7px;
    left: 30px;
    margin-left: -9px;
}


