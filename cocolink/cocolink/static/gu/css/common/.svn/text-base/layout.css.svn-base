/*
 ▼汎用 入力フォーム
--------------------------------------------------------------------------------*/
input[type="text"],
textarea,
select{
    width:100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-appearance: none;
    -o-appearance:none;
    -ms-appearance:none;
    border:none;
    outline:0;
    padding:0;
    margin:0;
}
textarea{
    resize:none;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    outline:0;
}

input[type="text"]:focus,
textarea:focus{
    outline: 0;
    border:none;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}
/*counter*/
.textarea_box{
    position: relative;
}
.textarea_box .counter{
    position: absolute;
    right: 10px;
    bottom: 10px;
}
.counter{
    color: #B2B2B2;
    font-size:86%;
}
.input_textarea{
    padding:10px;
}
.input_text[type="text"]{
    padding:13px 10px;
}
.input_text_style[type="text"],
textarea.input_text_style{
    background-color: #fff;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -webkit-box-shadow: 0 1px 0 white, inset 0 1px 0 1px rgba(0,0,0,0.1);
    -moz-box-shadow: 0 1px 0 white, inset 0 1px 0 1px rgba(0,0,0,0.1);
    box-shadow: 0 1px 0 white, inset 0 1px 0 1px rgba(0,0,0,0.1);
}
.input_pink_style{
    display: inline-block;
    background-color: #fcf5f5;
    border-radius: 5px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 0 rgba(255, 255, 255, 1);
}
.textarea_style{
    border:1px solid #c9bdbd;
    background:#fff;
    border-radius: 5px;
    -webkit-box-shadow: 0 2px 0 white, inset 0 1px 1px rgba(0,0,0,0.1);
    -moz-box-shadow: 0 2px 0 white, inset 0 1px 1px rgba(0,0,0,0.1);
    box-shadow: 0 2px 0 white, inset 0 1px 1px rgba(0,0,0,0.1);
}
.textarea_pink_style{
    background-color: #fcf5f5;
    border-radius: 5px;
    -webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
    -moz-box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
    box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
}
.textarea_white_style{
    background-color: #fff;
    border-radius: 5px;
    -webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
    -moz-box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
    box-shadow: 1px 1px 1px rgba(0, 0, 0, .2) inset , 0 1px 1px rgba(255, 255, 255, 1);
}

