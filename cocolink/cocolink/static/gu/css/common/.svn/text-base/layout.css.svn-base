	background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(226,225,225,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(226,225,225,1)));
