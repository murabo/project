@charset "utf-8";

/*
 outline
----------------------------------------------------------------------------------------------------*/
html{height:100%;}
body{
    position:relative;
    margin:0;
    padding:0;
    height:100%;
    background:#f7dedc;
    word-wrap:break-word;
    word-break: break-all;
}


/*テキスト
--------------------------------------------------------------------------------*/
body{
    font-family:HiraKakuProN-W3,Helvetica,sans-serif;
    font-size:14px;
    line-height:1.4;
    color:#000;
    -webkit-text-size-adjust:100%;
}

body,input,textarea,select{font-size:14px;}
body,input,select,textarea{color:#000;}

input{
    color:#000;
}
address{
font-style:normal;
}

/*
 element display for html5
----------------------------------------------------------------------------------------------------*/
*{
    -webkit-touch-callout:none;
}


/*リンク
--------------------------------------------------------------------------------*/
a{
    color:#000;
    text-decoration: none;
}
a:hover,
a:active{
    color:inherit;
}

.pganc{display:block;height:0;font-size:0;visibility:hidden;overflow:hidden;}

.linkarea{
    padding:10px;
    display:block;
    cursor:pointer;
}
.linkarea.pl0{
    padding-left:0;
}
.no_highlight,a.no_highlight{ -webkit-tap-highlight-color: rgba(0,0,0,0); }


/*フレーム
--------------------------------------------------------------------------------*/
.frame{
    position:relative;
    display:inline-block;
}
.frame:after{
    position: absolute;
    display: block;
    content: "";
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    -webkit-box-shadow: inset 1px 1px 1px rgba(0,0,0,0.2);
    -moz-box-shadow: inset 1px 1px 1px rgba(0,0,0,0.2);
    box-shadow: inset 1px 1px 1px rgba(0,0,0,0.2);
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius:4px;
}
.frame .official_edge_1{
    position:absolute;
    background:url(../../images/official01.png) 0 0 no-repeat;
    background-size:79px 230px;
    display:block;
}
.frame .img_size_35 + .official_edge_1{
    top:-1px;
    left:-1px;
    background-position:0 -151px;
    width:37px;
    height:37px;
}
.frame .img_size_48 + .official_edge_1{
    top:-2px;
    left:-2px;
    background-position:0 -89px;
    width:52px;
    height:52px;
}

