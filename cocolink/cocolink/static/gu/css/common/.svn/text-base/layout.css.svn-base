/*ラジオボタンchk のスタイル*/
.radio_chk input{
    opacity:0;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
.radio_chk input:checked + label {
    background:url(data:image/png;base64,) no-repeat;
    background-size:13px 11px;
    width:13px;
    height:11px;
    padding-left:15px;
    font-weight:bold;
    text-shadow:0 1px 0 rgba(255,255,255,0.6);
}

/*ラジオボタン▼ のスタイル*/
.radio_toggle input{
    opacity:0;
}
.radio_toggle label{
    background:url(data:image/png;base64,) no-repeat;
    background-size:15px 15px;
    width:15px;
    height:22px;
    margin-top:10px;
    padding:0 0 0 18px;
    text-shadow:0 1px 0 rgba(255,255,255,0.6);
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
.radio_toggle input:checked + label {
    background:url(data:image/png;base64,) no-repeat;
    background-size:15px 15px;
    font-weight:bold;
}

