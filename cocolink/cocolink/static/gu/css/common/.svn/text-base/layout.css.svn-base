/*
 ▼汎用 ボタン
--------------------------------------------------------------------------------*/
input[type="submit"]{
    display:block;
    width:100%;
    border:none;
}
.btn span,btn a{
    cursor:pointer;
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}
.btn{
    border:none;
    height:28px;
    line-height:28px;
    padding:0 8px;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    font-weight:bold;
}
.btn.comment_btn{
    padding:0 4px;
}
.btn.mini{
    padding:0;
    width:36px;
    text-align:center;
    color:#807979;
}
.btn2{
    height:44px;
    line-height:44px;
    padding:0 8px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    font-weight:bold;
    text-align:center;
}
a.a.btn,
a.btn.mini,
a.btn2{
    display:block;
}
.btn2.mini{
    display:block;
    height:34px;
    line-height:33px;
}
.btn2.edit_profile{
    line-height:normal;
    padding:9px 0;
}
.btn.mini25{
    display:block;
    height:25px;
    line-height:24px;
}

.btn2.p0{
    padding:0;
}
.btn2.login{
    padding:0 10px;
    vertical-align:middle;
    line-height:32px;
}

.btn3{
    position: relative;
    display:block;
    height:25px;
    line-height:25px;
    min-width:24px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    font-weight:bold;
    text-align:center;
}
.btn4{
    display:block;
    height:40px;
    line-height:40px;
    padding:0 8px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    font-weight:bold;
    text-align:center;
}
.btn5{
    text-align:center;
    font-size:10px;
    width:90px;
    height:30px;
    line-height:30px;
    display:block;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    display:block;
    -webkit-box-shadow: inset 0px -3px 0px rgba(0, 0, 0, 0.4);
    box-shadow: inset 0px -3px 0px rgba(0, 0, 0, 0.2);
}
.btn_arrow_l{
    position:absolute;
    top:50%;
    left:6px;
    margin-top:-6px;
}
.btn_arrow_r{
    position:absolute;
    top:50%;
    right:6px;
    margin-top:-6px;
}
.btn_all{width:100%;}




